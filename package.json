{
  "name": "fluxtheme",
  "version": "2.0.0",
  "description": "fluxus theme generator or something",
  "main": "index.ts",
  "scripts": {
    "start": "node -r ts-node/register ./src/index.ts",
    "dev": "nodemon",
    "update": "ts-node src/index.ts",
    "build": "tsc"
  },
  "author": "YieldingExploiter",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.27.2",
    "discord-api-types": "^0.36.3",
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "gm": "^1.23.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@norathefurry/eslint-config": "1.1.2",
    "@types/fs-extra": "9.0.13",
    "@types/gm": "1.18.12",
    "@types/node": "18.6.4",
    "eslint": "8.21.0",
    "nodemon": "2.0.19",
    "tslib": "2.4.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
